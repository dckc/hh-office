# see also hh-office.conf

<FilesMatch "\.ini$">
Deny from all
</FilesMatch>

RewriteEngine on
RewriteRule print_report/(.*) report_gen.py/$1
RewriteRule export_claims export_claims.py
RewriteRule upload_insurance upload_insurance.py

<Files report_gen.py>
  SetHandler cgi-script
  Options +ExecCGI
</Files>

<Files export_claims.py>
  SetHandler cgi-script
  Options +ExecCGI
</Files>
